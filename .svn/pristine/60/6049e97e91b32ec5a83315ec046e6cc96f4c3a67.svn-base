package br.org.iamar.firebase.model;

import java.util.Map;

public class Profile {

	private String code;
	private String name;
	private Map<String, String> displayGroup;
	private Map<String, String> postingGroup;
	private Boolean needApproval;
	private String region;
	private Map<String, String> classes;
	private Map<String, String> projects;

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Map<String, String> getDisplayGroup() {
		return displayGroup;
	}

	public void setDisplayGroup(Map<String, String> displayGroup) {
		this.displayGroup = displayGroup;
	}

	public Map<String, String> getPostingGroup() {
		return postingGroup;
	}

	public void setPostingGroup(Map<String, String> postingGroup) {
		this.postingGroup = postingGroup;
	}

	public Boolean getNeedApproval() {
		return needApproval;
	}

	public void setNeedApproval(Boolean needApproval) {
		this.needApproval = needApproval;
	}

	public String getRegion() {
		return region;
	}

	public void setRegion(String region) {
		this.region = region;
	}

	public Map<String, String> getClasses() {
		return classes;
	}

	public void setClasses(Map<String, String> classes) {
		this.classes = classes;
	}

	public Map<String, String> getProjects() {
		return projects;
	}

	public void setProjects(Map<String, String> projects) {
		this.projects = projects;
	}

	@Override
	public String toString() {
		return "Profile [code=" + code + ", displayGroup=" + displayGroup + ", postingGroup=" + postingGroup
				+ ", needApproval=" + needApproval + ", region=" + region + ", classes=" + classes + ", projects="
				+ projects + "]";
	}

}
