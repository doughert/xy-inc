<div class="page-header">
	<div class="container">
		<div class="row">
			<div class="col-md-6">
				<h1>{{profile.name}}</h1>
			</div>
			<div class="col-md-6 pull-right btn-lg">
				<button type="button" class="btn btn-primary btn-lg pull-right" ng-click="save()">Salvar</button>
			</div>
		</div>
		<table ng-table="tableParams" class="table table-condensed table-bordered table-striped table-hover" show-filter="true">
			<thead>
				<tr>
					<th>Associar?</th>
					<th>Id</th>
					<th>Nome</th>
				</tr>
			</thead>
			<tr ng-repeat="feature in $data">
				<td>
					<input type="checkbox" ng-checked="feature.isRowSelected" ng-model="selectedFeatures[feature.id]" />
				</td>
				<td title="Id" filter="{ id: 'number'}" sortable="'id'">
					{{feature.id}}</td>
				<td title="Nome" filter="{ name: 'text'}" sortable="'name'">
					{{feature.name}}</td>
			</tr>
		</table>
	</div>
</div>