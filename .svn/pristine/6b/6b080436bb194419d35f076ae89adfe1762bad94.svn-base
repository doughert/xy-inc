<div class="page-header">
	<div class="container">
	   <table ng-table="tableParams" class="table table-condensed table-bordered table-striped table-hover" show-filter="true">
			<thead>
				<tr>
					<th>Id</th>
					<th>Nome</th>
					<th>Associar Funcionalidades</th>
					<th>Associar Perfis de Vis√£o</th>
				</tr>
			</thead>
			<tr ng-repeat="profile in $data">
				<td title="Id" filter="{ id: 'number'}" sortable="'id'">
					{{profile.id}}</td>
				<td title="'Nome'" filter="{ name: 'text'}" sortable="'name'">
					{{profile.name}}</td>
				<td style="text-align: center">
					<button type="button" class="btn btn-primary" ng-click="redirectFeatureAssociation(profile.id)">Funcionalidades</button>
				</td>
				<td style="text-align: center">
					<button type="button" class="btn btn-primary" ng-click="redirectProfileAssociation(profile.id)">Pode Ver</button>
				</td>
			</tr>
		</table>
	</div>
</div>
