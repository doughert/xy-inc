<div class="page-header">
	<div class="container">
		<table ng-table="tableParams" class="table table-condensed table-bordered table-striped" show-filter="true">
			<thead>
				<tr>
					<th>Id</th>
					<th>Nome</th>
					<th>Documento</th>
					<th>Registro</th>
					<th>Compania</th>
					<th>Direção</th>
					<th>Departamento</th>
					<th>Data de Aniversário</th>
					<th>Usuário Zape</th>
					<th>Senha Zape</th>
				</tr>
			</thead>
			<tr ng-repeat="user in $data" ng-click="redirectUserEdit(user.id)">
				<td title="Id" filter="{ id: 'number'}" sortable="'id'">
					{{user.id}}</td>
				<td title="Nome" filter="{ name: 'text'}" sortable="'name'">
					{{user.name}}</td>
				<td title="Documento" filter="{ documentNumber: 'text'}" sortable="'documentNumber'">
					{{user.documentNumber}}</td>
				<td title="Registro" filter="{ registration: 'text'}" sortable="'registration'">
					{{user.registration}}</td>
				<td title="Compania" filter="{ company: 'text'}" sortable="'company'">
					{{user.company}}</td>
				<td title="Direção" filter="{ direction: 'text'}" sortable="'direction'">
					{{user.direction}}</td>
				<td title="Departamento" filter="{ department: 'text'}" sortable="'department'">
					{{user.department}}</td>
				<td title="Data de Aniversário" filter="{ dateOfBirth: 'date'}" sortable="'dateOfBirth'">
					{{user.dateOfBirth}}</td>
				<td title="Usuário Zape" filter="{ zapeUser: 'text'}" sortable="'zapeUser'">
					{{user.zapeUser}}</td>
				<td title="Senha Zape" filter="{ zapePassword: 'text'}" sortable="'zapePassword'">
					{{user.zapePassword}}</td>
			</tr>
		</table>
	</div>
</div>
