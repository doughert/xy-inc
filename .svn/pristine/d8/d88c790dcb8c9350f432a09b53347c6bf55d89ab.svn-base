package br.org.iamar.firebase.model;

import java.util.Map;

public class Project {

	private String name;
	private String description;
	private String institutionName;
	private Map<String, String> classes;

	public Project() {
	}

	public Project(String name, String description, String institutionName) {
		this.name = name;
		this.description = description;
		this.institutionName = institutionName;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getInstitutionName() {
		return institutionName;
	}

	public void setInstitutionName(String institutionName) {
		this.institutionName = institutionName;
	}

	public Map<String, String> getClasses() {
		return classes;
	}

	public void setClasses(Map<String, String> classes) {
		this.classes = classes;
	}

	@Override
	public String toString() {
		return "Project [name=" + name + ", description=" + description + ", institutionName=" + institutionName
				+ ", classes=" + classes + "]";
	}

}
