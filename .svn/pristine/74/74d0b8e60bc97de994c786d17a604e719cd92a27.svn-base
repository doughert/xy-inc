package br.org.iamar.sgci.model;


import javax.persistence.Entity;
import javax.persistence.FetchType;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;


@Entity( name = "view_post" )
public class ViewPost {

    @Id
    @GeneratedValue( strategy = GenerationType.IDENTITY )
    private Long id;

    @ManyToOne( fetch = FetchType.LAZY )
    @JoinColumn( name = "profile_viewer", referencedColumnName = "id", nullable = false )
    private Profile profileViewer;

    @ManyToOne( fetch = FetchType.LAZY )
    @JoinColumn( name = "profile_poster", referencedColumnName = "id", nullable = false )
    private Profile profilePoster;


    public Long getId() {

        return id;
    }


    public void setId( Long id ) {

        this.id = id;
    }


    public Profile getProfileViewer() {

        return profileViewer;
    }


    public void setProfileViewer( Profile profileViewer ) {

        this.profileViewer = profileViewer;
    }


    public Profile getProfilePoster() {

        return profilePoster;
    }


    public void setProfilePoster( Profile profilePoster ) {

        this.profilePoster = profilePoster;
    }

}
